{
  "name": "@ibrahimstudio/button",
  "version": "1.1.5",
  "description": "This package provides a customizable button component for React applications.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "workspaces": [
    "../../utils/*",
    "../../components/*"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest",
      "^.+\\.css$": "jest-css-modules-transform"
    },
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js|jsx)",
      "**/?(*.)+(spec|test).+(ts|tsx|js|jsx)"
    ]
  },
  "keywords": [
    "button",
    "react",
    "typescript"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/space-ibrahimstudio/ibrahimstudio.git",
    "directory": "packages/components/button"
  },
  "bugs": {
    "url": "https://github.com/space-ibrahimstudio/ibrahimstudio/issues"
  },
  "author": "Ibrahim Space Studio",
  "license": "MIT",
  "homepage": "https://space.ibrahimstudio.co",
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "react-router-dom": ">=6"
  },
  "devDependencies": {
    "@ibrahimstudio/jsx": "workspace:*",
    "@ibrahimstudio/styles": "workspace:*",
    "@ibrahimstudio/hooks": "workspace:*",
    "@ibrahimstudio/tooltip": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
